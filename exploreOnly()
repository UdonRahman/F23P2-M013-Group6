import random

m1 = [7, 4, 10, 5]
d1 = [3, 10, 6, 2]


def h(p1: int, p2: int) -> float:  # get a random happiness
    happy = random.normalvariate(p1, p2)
    return happy


def exploreOnly(p1: list, p2: list) -> int:
    result = 0

    for index in range(len(p1)):
        one_cafe = 50
        while one_cafe > 0:
            h2 = h(p1[index], p2[index])
            result = result + h2
            one_cafe = one_cafe - 1
    return result


print(exploreOnly(m1, d1))

